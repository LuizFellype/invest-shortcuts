{
  "manifest_version": 3,
  "name": "Invest Shortcuts",
  "version": "0.4",
  "background.service_worker": {
    "persistent": false,
    "scripts": [
      "content.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://qxbroker.com/pt/trade*",
        "https://pocketoption.com/en/cabinet/*"
      ],
      "js": [
        "graphics.js",
        "timeframe.js",
        "createButton.js",
        "expiration.js",
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}